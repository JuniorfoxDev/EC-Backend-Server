{
  "version" : 2,
  "builds": [
    {
       "src": "*.js", 
      "use": "@vercel/node", 
      "config": {
        "installCommand": "npm install --legacy-peer-deps",
        "maxLambdaSize": "50mb"
      }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/index.js" 
    }
  ]
}
